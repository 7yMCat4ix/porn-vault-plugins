{
  "name": "profile_pics",
  "version": "1.0.1",
  "authors": ["boi123212321"],
  "description": "Find actor images based on local files",
  "arguments": [
    {
      "name": "path",
      "type": "String",
      "required": true,
      "description": "Folder to search images in"
    },
    {
      "name": "target",
      "type": "String",
      "required": false,
      "default": "thumbnail",
      "description": "Actor image to set (possible values: 'thumbnail', 'altThumbnail', 'avatar', 'hero')"
    }
  ]
}
